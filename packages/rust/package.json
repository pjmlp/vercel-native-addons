{
    "name": "hello-rust",
    "version": "1.0.0",
    "author": "Paulo Pinto",
    "description": "An example of a Rust addon package",
    "main": "hello.mjs",
    "private": true,
    "engines": {
        "node": ">=18",
        "npm": "Please use Yarn"
    },
    "license": "GPL-2.0-or-later",
    "scripts": {
        "build": "cargo-cp-artifact -a cdylib hello lib/hello.node -- cargo build --message-format=json-render-diagnostics",
        "build-debug": "npm run build --",
        "build-release": "npm run build -- --release",
        "install": "npm run build-release",
        "test": "cargo test && node test/testmodule.mjs"
      },
    "devDependencies": {
      "node-loader": "^2.0.0",
      "typescript": "4.9.3",
      "cargo-cp-artifact": "^0.1"
    }
  }
  